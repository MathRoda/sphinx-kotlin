<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/episode_detail_nav_graph"
    app:startDestination="@+id/navigation_episode_detail_fragment">

    <fragment
        android:id="@+id/navigation_episode_detail_fragment"
        android:name="chat.sphinx.episode_detail.ui.EpisodeDetailFragment"
        tools:layout="@layout/fragment_episode_detail">
        <argument
            android:name="argFeedId"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
        <argument
            android:name="argHeader"
            app:argType="string" />
        <argument
            android:name="argImage"
            app:argType="string" />
        <argument
            android:name="argEpisodeTypeImage"
            app:argType="integer" />
        <argument
            android:name="argEpisodeTypeText"
            app:argType="string" />
        <argument
            android:name="argEpisodeDate"
            app:argType="string" />
        <argument
            android:name="argEpisodeDuration"
            app:argType="string" />
        <argument
            android:name="argDownloaded"
            app:argType="boolean"/>
        <argument
            android:name="argLink"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>

    </fragment>

</navigation>
