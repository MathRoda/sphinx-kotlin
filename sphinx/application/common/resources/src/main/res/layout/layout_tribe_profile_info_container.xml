<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/layout_tribe_profile_info"
    android:background="@color/headerBG"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_layout_tribe_row_1"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_price_to_meet_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:inputType="textCapWords"
            android:singleLine="true"
            android:text="@string/tribe_member_profile_price_to_meet"
            android:textColor="@color/secondaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Posts:" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_price_to_meet"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:maxLines="2"
            android:text=""
            android:textColor="@color/primaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            android:layout_marginStart="@dimen/default_layout_margin"
            android:gravity="right"
            android:ellipsize="end"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/text_view_price_to_meet_label"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="150" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint_layout_tribe_name_badges_border"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/body"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_layout_tribe_row_2"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraint_layout_tribe_row_1">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_coding_languages_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:inputType="textCapWords"
            android:singleLine="true"
            android:text="Coding languages:"
            android:textColor="@color/secondaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_coding_languages"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:text=""
            android:textColor="@color/primaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            android:gravity="right"
            android:ellipsize="end"
            android:maxLines="2"
            android:layout_marginStart="@dimen/default_layout_margin"
            app:layout_constraintStart_toEndOf="@+id/text_view_coding_languages_label"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Python, Swift, Java, Golang, Python, swift" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/body"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_layout_tribe_row_3"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraint_layout_tribe_row_2">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_posts_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:inputType="textCapWords"
            android:singleLine="true"
            android:text="@string/tribe_member_profile_post"
            android:textColor="@color/secondaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Posts:" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_posts"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:maxLines="2"
            android:text=""
            android:textColor="@color/primaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            android:gravity="right"
            android:ellipsize="end"
            android:layout_marginStart="@dimen/default_layout_margin"
            app:layout_constraintStart_toEndOf="@+id/text_view_posts_label"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="150" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/body"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_layout_tribe_row_4"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraint_layout_tribe_row_3">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_twitter_account_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:inputType="textCapWords"
            android:singleLine="true"
            android:text="@string/tribe_member_profile_twitter"
            android:textColor="@color/secondaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Contributions:" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_twitter_account"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:maxLines="2"
            android:text=""
            android:textColor="@color/primaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            android:gravity="right"
            android:ellipsize="end"
            android:layout_marginStart="@dimen/default_layout_margin"
            app:layout_constraintStart_toEndOf="@+id/text_view_twitter_account_label"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="ttiminskas" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/body"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_layout_tribe_row_5"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        app:layout_constraintTop_toBottomOf="@+id/constraint_layout_tribe_row_4">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_github_account_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:inputType="textCapWords"
            android:singleLine="true"
            android:text="@string/tribe_member_profile_github"
            android:textColor="@color/secondaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Earnings:" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/text_view_github_account"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:maxLines="2"
            android:text=""
            android:textColor="@color/primaryText"
            android:textSize="@dimen/default_form_edit_view_text_size"
            android:gravity="right"
            android:ellipsize="end"
            android:layout_marginStart="@dimen/default_layout_margin"
            app:layout_constraintStart_toEndOf="@+id/text_view_github_account_label"
            app:backgroundTint="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="ttiminskas" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/body"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>